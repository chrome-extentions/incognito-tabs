{
  "manifest_version": 3,
  "name": "Incognito Tab Saver",
  "version": "1.0",
  "permissions": [
    "tabs",
    "windows",
    "storage",
    "sessions",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "incognito": "split"
}
